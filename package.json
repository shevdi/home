{
  "name": "shevdi-home",
  "version": "0.0.0",
  "description": "shevdi home",
  "type": "module",
  "scripts": {
    "build": "npm run build --prefix client",
    "dev": "concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "start": "concurrently \"npm run start --prefix server\" \"npm run start --prefix client\"",
    "typecheck": "react-router typegen && tsc",
    "lint": "docker-compose exec home-frontend npm run lint",
    "docker:build": "docker compose build",
    "docker:up:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down"
  },
  "keywords": [
    "nodejs",
    "express",
    "docker",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv-webpack": "^8.1.1",
    "nodemon": "^3.0.2",
    "typescript": "^5.8.3",
    "webpack": "^5.101.0"
  }
}
